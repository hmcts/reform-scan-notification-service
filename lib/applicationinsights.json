{
  "connectionString": "${file:/mnt/secrets/reform-scan/app-insights-connection-string}",
  "role": {
    "name": "reform-scan-notification-service"
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "name",
              "value": ".*\\.run.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "percentage": 0
        },
        {
          "telemetryType": "trace",
          "percentage": 0
        }
      ]
    }
  }
}
